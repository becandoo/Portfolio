<section id="contact">
  <div class="grid contact">
    <div class="columns-12">
      <h2>Contact</h2>
      <hr />
    </div>
  </div>
  <div class="grid">
    <div class="columns-6">
        <iframe class="google-map" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d449479.1575398344!2d-122.39055670334066!3d45.68848714595324!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x54950b0b7da97427%3A0x1c36b9e6f6d18591!2sPortland%2C+OR!5e0!3m2!1sen!2sus!4v1450716891037"  frameborder="0" style="border:0" allowfullscreen></iframe>
    </div>
    <div class="columns-6">
                    <form id="contactform" action="//formspree.io/brandoncranmore@gmail.com" method="post">
                        <div class="ajax-hidden">
                            <div style="visibility: visible;">
                                <input name="name" class="form-control" id="name" type="text" placeholder="Name">
                            </div>
                            <div style="visibility: visible;" data-wow-delay=".1s">
                                <input name="email" class="form-control" id="email" type="email" placeholder="E-mail">
                            </div>
                            <div style="visibility: visible;" data-wow-delay=".2s">
                                <textarea name="comment" class="form-control" id="comment" placeholder="Message" rows="7"></textarea>
                            </div>
                            <button class="btn btn-lg btn-block" style="visibility: visible;" type="submit" value="Send Message" data-wow-delay=".3s">Send Message</button>
                        </div>
                        <div class="ajax-response"></div>
                    </form>
                </div>
      </div>

</section>
